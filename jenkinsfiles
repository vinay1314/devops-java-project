pipeline
{   
        agent any
        stages
        {       
                stage('Code Checkout')
                {
                       steps
                       {
                            git 'https://github.com/vinay1314/devops-java-project.git'
                            
                       }
                }
                stage('Code Compile')
                {
                        steps
                        {
                              sh 'mvn compile'
                        
                        }
                }
                stage('Test')
                {
                        steps
                        {
                              sh 'mvn test'
                              
                        }
                }
                stage('Packaging')
                {
                        steps
                        {
                              sh 'mvn package'
                        }      
                }
        }
}
